# batch size
batch_size: 1

# feature dimemtion for reading the image
feature_row: 256
feature_col: 256
feature_ph_row: 224
feature_ph_col: 224
feature_cha: 3

# the croped image dimention as the input for the network

label_row: 256
label_col: 256

# the croped image size as the input for the network
label_ph_row: 224
label_ph_col: 224

label_cha: 1

mask_row: 256
mask_col: 256
mask_ph_row: 224
mask_ph_col: 224
mask_cha: 1

leaky_param:0.01
weight_decay:0.0004

data_arg.rflip_leftright: True
data_arg.rcrop_size: [224, 224]

train_file_name:../file_list/test_file2.txt
test_file_name:../file_list/test_file2.txt

init_learning_rate: 0.0001
max_training_iter:10
test_per_iter:10
train_log_dir:logs/
model_dir:models/
restore_model:False

